@using ZeroHunger.Models
@{
    ViewBag.Title = "Profile";
}



@{
    RestaurantModel rm = new RestaurantModel();
    rm = (RestaurantModel)Session["rs"];
}





<div class="card text-center">
    <div class="card-header">
        Profile
        <div>
            <img src="~/rs.png" class="rounded-circle" width="80px" height="100px">
        </div>
    </div>
    <div class="card-body">
     
        <p class="card-text">Restaurant Name : @rm.ResName </p>
        <p class="card-text">Location        : @rm.ResLocation</p>
        <p class="card-text">User Name       : @rm.ResUname</p>

    </div>
</div>